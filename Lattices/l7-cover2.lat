; l7-cover.lat

;;; The unique ji cover of V(L_7), see Rose's monograph.

(in-package :user)


(setq l7-cover (make-finite-lattice
    ;; '(0 1 a b b_0 b1 c d e f g h i j k )
    ;; '(z t a b m   n c d e f g h i j k )
    '(
      (z (e n))
      (a (c))
      (b (f))
      (m (g))
      (n (h))
      (c (f g h))
      (d (a b))
      (e (d m))
      (f (i j))
      (g (i k))
      (h (j k))
      (i (t))
      (j (t))
      (k (t))
      (t ())  ) ) )

